import datetime

# create two datetime objects
date1 = datetime.datetime(2023, 2, 20, 12, 30, 0)
date2 = datetime.datetime(2023, 2, 22, 8, 45, 15)

# calculate the difference in seconds
difference_seconds = abs((date2 - date1).total_seconds())

# print the difference in seconds
print("The difference between the two dates is", difference_seconds, "seconds.")
